@using Microsoft.AspNet.Identity;
@{
    ViewBag.Title = "Franquias | Portal APCD";
}
@section breadcrumb{
    <!-- Breadcrumb-->
    <ol class="breadcrumb">
        <li class="breadcrumb-item">
            <a href="@Url.Action("Index", "Home")">Dashboard</a>
        </li>
        <li class="breadcrumb-item active">Franquias</li>
        <!-- Breadcrumb Menu-->
        <li class="breadcrumb-menu d-md-down-none">
            <div class="btn-group" role="group" aria-label="Button group">
                <a class="btn" href="javascript:void(0)">
                    Olá @User.Identity.Name !
                </a>
            </div>
        </li>
    </ol>
}
    <div class="row">
        <iframe width="100%" height="800px" src="http://franquias.portalzeus.com.br/franquias?iduser=@User.Identity.GetUserId()" frameborder="0" allowfullscreen></iframe>
    </div>
