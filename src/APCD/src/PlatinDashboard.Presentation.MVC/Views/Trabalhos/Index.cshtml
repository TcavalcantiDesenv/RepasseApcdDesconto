
@{
    ViewBag.Title = "Trabalhos | Portal APCD";
}
@section styles{
    @Styles.Render("~/coreui/ladda/css")
    @Styles.Render("~/coreui/datatables/css")
}
@section breadcrumb{
    <!-- Breadcrumb-->
    <ol class="breadcrumb">
        <li class="breadcrumb-item">
            <a href="@Url.Action("Index", "Home")">Painel Geral</a>
        </li>
        <li class="breadcrumb-item active">Trabalhos e Pranchas</li>
        <!-- Breadcrumb Menu-->
        <li class="breadcrumb-menu d-md-down-none">
            <div class="btn-group" role="group" aria-label="Button group">
                <a class="btn" href="javascript:void(0)">
                    Olá @User.Identity.Name !
                </a> 
            </div>
        </li>
    </ol>
}

    <div class="card">
        <div class="card-header">
            <h4 style="display: inline-block">Trabalhos e Pranchas</h4>
        </div>
        @*<iframe src="http://hiltonslima-001-site9.btempurl.com/Trabalhos?usuario=" @ViewBag.ListaGraduacao width="1243" height="600"></iframe>*@
        <iframe src=@ViewBag.ListaGraduacao  frameborder="0" marginheight="0" marginwidth="0" scrolling="no" width="900" height="400"></iframe>
    </div>

@section modals{
    <div id="modal-section"></div>
}
@section scripts{
    @Scripts.Render("~/bundles/unobstrusive")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/coreui/ladda/js")
    @Scripts.Render("~/coreui/datatables/js")
    <script src="~/Scripts/viewsjs/index.js"></script>
    <script src="~/Scripts/viewsjs/videoindex.js"></script>
    <script src="~/Scripts/viewsjs/changeclient.js"></script>
    <script>
        $(document).ready(function () {
            loadClientes();
            initializeVideoTable();
        });
    </script>
}



