@model List<Model.Entity.Familiar>
@{
    ViewBag.Title = $"Usuário   | Portal APCD";
}
@section styles{
    @Styles.Render("~/coreui/ladda/css")
    @Styles.Render("~/coreui/select2/css")
    <style>
        .select2-selection {
            border: 1px solid #e4e7ea !important;
        }

        .locked-tag .select2-selection__choice__remove {
            display: none !important;
        }
    </style>
}
@section breadcrumb{
    <!-- Breadcrumb-->
    <ol class="breadcrumb">
        <li class="breadcrumb-item">
            <a href="@Url.Action("Index", "Home")">Painel Geral</a>
        </li>
        <li class="breadcrumb-item active">Excluir Familiar Maçon</li>
        <!-- Breadcrumb Menu-->
        <li class="breadcrumb-menu d-md-down-none">
            <div class="btn-group" role="group" aria-label="Button group">
                <a class="btn" href="javascript:void(0)">
                    Olá @User.Identity.Name !
                </a>
            </div>
        </li>
    </ol>
}

<div class="card">
    <div class="card-header">
        <h4 style="display: inline-block">Deseja Excluir o familiar  @ViewBag.Nome ?</h4>
        <a href="@Url.Action("DeleteConfirmado", "Familiar", new { id = ViewBag.ID })" class="btn btn-danger pull-right">Excluir</a>
    </div>
    <div class="row">
        <div class="form-group  col-sm-12">
            <a href="@Url.Action("Index", "Familiar", new { id = ViewBag.ID })" class="btn btn-success pull-right">Voltar</a>
        </div>
    </div>
</div>
@section scripts{
    @Scripts.Render("~/bundles/unobstrusive")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/coreui/ladda/js")
    @Scripts.Render("~/coreui/select2/script")
    <script src="~/Scripts/viewsjs/user.js"></script>
    <script src="~/Scripts/viewsjs/details.js"></script>
    <script src="~/Scripts/viewsjs/userdetails.js"></script>
}





